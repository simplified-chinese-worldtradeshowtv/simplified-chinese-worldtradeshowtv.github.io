		<link>/contents/44471/</link>
					<comments>/contents/44471/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Fri, 03 Mar 2023 07:07:36 +0000</pubDate>
				<category><![CDATA[movie]]></category>
		<category><![CDATA[表面设计展 2023]]></category>
