		<media:content url="/www.tenji.tv/wp-content/uploads/video_img/japan-drone_2022/japan-drone_2022_06-1.jpg" medium="image" />
		<media:content url="/www.tenji.tv/wp-content/uploads/video_img/japan-drone_2022/japan-drone_2022_06-2.jpg" medium="image" />
		<media:content url="/www.tenji.tv/wp-content/uploads/video_img/japan-drone_2022/japan-drone_2022_06-3.jpg" medium="image" />
