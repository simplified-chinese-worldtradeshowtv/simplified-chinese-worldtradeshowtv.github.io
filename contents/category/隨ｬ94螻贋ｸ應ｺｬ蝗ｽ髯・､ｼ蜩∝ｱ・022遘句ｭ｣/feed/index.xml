					<comments>/contents/42064/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Thu, 29 Sep 2022 03:31:17 +0000</pubDate>
				<category><![CDATA[movie]]></category>
